# Test environment variables for TofuSoup
# Source this file before running tests: source .env.test

# CTY test suite environment variables
export TOFUSOUP_TEST_DEFAULT_ENV="cty_override_default_env_from_toml"
export TOFUSOUP_CTY_SUITE_ENV="cty_specific_env_value_from_toml"

# Harness and plugin environment
export PLUGIN_MAGIC_COOKIE_KEY="BASIC_PLUGIN"
export PLUGIN_MAGIC_COOKIE_VALUE="hello"
export PLUGIN_PROTOCOL_VERSIONS="1"
export PLUGIN_AUTO_MTLS="false"

# Logging
export TOFUSOUP_LOG_LEVEL="INFO"
export LOG_LEVEL="INFO"

# Go environment
export GODEBUG="asyncpreemptoff=1,panicasync=1"
export PYTHONUNBUFFERED="1"